{
    "name": "perfect-code/module-product-media-uploader",
    "description": "This module for Magento 2 helps to download an image from external URL for further save it as a product media gallery entry.",
    "require": {
        "php": "^7.4|^8",
        "ext-json": "*",
        "laminas/laminas-validator": "*",
        "magento/framework": "*",
        "magento/module-catalog": "*",
        "magento/module-config": "*",
        "magento/magento-composer-installer": "*"
    },
    "suggest": {

    },
    "type": "magento2-module",
    "version": "1.0.4",
    "license": [
        "GPL-3.0"
    ],
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "PerfectCode\\ProductMediaUploader\\": ""
        }
    },
    "extra": {
        "map": [
            [
                "*",
                "PerfectCode/ProductMediaUploader"
            ]
        ]
    }
}
